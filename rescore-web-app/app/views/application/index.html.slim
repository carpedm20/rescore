.row
  .large-12.columns
    table
      -@movies.each do |movie|
        tr
          td =image_tag(movie.image_url)
          td =link_to movie.title, movie_path(movie)
          td =high_chart("#{movie.id}-ratings", movie.ratings_chart)
          td =high_chart("#{movie.id}-topics", movie.topics_chart)